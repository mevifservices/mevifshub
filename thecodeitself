local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Mevif's Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

-- rejoin       
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local Rejoin = coroutine.create(function()
	local Success, ErrorMessage = pcall(function()
		TeleportService:Teleport(game.PlaceId, LocalPlayer)
	end)

	if ErrorMessage and not Success then
		warn(ErrorMessage)
	end
end)


local Tab = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Movement"
})

Tab:AddSlider({
	Name = "Walkspeed",
	Min = 1,
	Max = 500,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})

Tab:AddSlider({
	Name = "Jump Height",
	Min = 1,
	Max = 1000,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Height",
	Callback = function(jump)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = jump
		game.Players.LocalPlayer.Character.Humanoid.JumpHeight = jump
	end 
})

Tab:AddButton({
	Name = "Morning",
	Callback = function()
		game.Lighting.ClockTime = 12
	end    
})

Tab:AddButton({
	Name = "Night",
	Callback = function()
		game.Lighting.ClockTime = 24
	end    
})

Tab:AddButton({
	Name = "Use gravity with chat! (Example: g50) Default Gravity is 196.2",
	Callback = function()
		-- server script (ServerScriptService) 
		local function onChatted(player, message)
			local gravityPrefix = "g"
			if string.sub(message, 1, #gravityPrefix) == gravityPrefix then
				local newGravity = tonumber(string.sub(message, #gravityPrefix + 1))
				if newGravity then
					game.Workspace.Gravity = newGravity
				end
			end
		end

		for _, player in pairs(game:GetService("Players"):GetPlayers()) do
			player.Chatted:Connect(function(message) onChatted(player, message) end)
		end

		game:GetService("Players").PlayerAdded:Connect(function(player)
			player.Chatted:Connect(function(message) onChatted(player, message) end)
		end)

	end    
})

Tab:AddButton({
	Name = "Walk on Walls (Execute only one time unless reset!)",
	Callback = function()
		loadstring(game:HttpGet("https://pastebin.com/raw/zXk4Rq2r"))()
	end    
})

Tab:AddButton({
	Name = "Reset",
	Callback = function()
		local plr = game:GetService("Players").LocalPlayer
		local character = plr.Character or plr.CharacterAdded:Wait()
		wait(0.1)
		local humanoid = character:FindFirstChild("Humanoid")

		humanoid.Health = 0

	end    
})

Tab:AddButton({
	Name = "Workspace Camera (Client)",
	Callback = function()
		game:GetService("Workspace").CurrentCamera.CameraType = Enum.CameraType.Scriptable
	end    
})


Tab:AddButton({
	Name = "120 FOV",
	Default = false,
	Callback = function(Value)
		local runService=game:GetService("RunService")
		local camera=workspace.Camera
		local fov=120
		runService.RenderStepped:Connect(function()
			camera.FieldOfView=fov
			if fov>=120 then
				local dv=(1.7320508075688767*((camera.ViewportSize.Y/2)/math.tan(math.rad(fov/2))))/(camera.ViewportSize.Y/2)
				camera.CFrame*=CFrame.new(0,0,0,dv,0,0,0,dv,0,0,0,1)
			end
		end)

	end    
})

Tab:AddButton({
	Name = "Infinite Jump",
	Default = false,
	Callback = function(Value)
		local infjmp = true
		game:GetService("UserInputService").jumpRequest:Connect(function()
			if infjmp then
				game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass"Humanoid":ChangeState("Jumping")
			end
		end)
	end
})

Tab:AddButton({
	Name = "Teleport Tool",
	Default = false,
	Callback = function(Value)
		local mouse = game.Players.LocalPlayer:GetMouse()
		local tool = Instance.new("Tool")
		tool.RequiresHandle = false
		tool.Name = "Teleport Tool"
		tool.Activated:connect(function()
			local pos = mouse.Hit+Vector3.new(0,2.5,0)
			pos = CFrame.new(pos.X,pos.Y,pos.Z)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
		end)
		tool.Parent = game.Players.LocalPlayer.Backpack
	end    
})

Tab:AddButton({
	Name = "X-Ray (Press X to toggle)",
	Default = false,
	Callback = function(Value)
		local t=false

		local function scan(z,t)
			for _,i in pairs(z:GetChildren()) do
				if i:IsA("BasePart") and not i.Parent:FindFirstChild("Humanoid") and not i.Parent.Parent:FindFirstChild("Humanoid") then
					i.LocalTransparencyModifier=t
				end

				scan(i,t)
			end
		end

		local function X(v)
			if v then
				scan(workspace,0.5)
			else
				scan(workspace,0)
			end
		end

		game:GetService("UserInputService").InputBegan:connect(function (input)
			if input.UserInputType==Enum.UserInputType.Keyboard and input.KeyCode==Enum.KeyCode.X then
				t=not t
				X(t)
			end
		end)
	end    
})

Tab:AddButton({
	Name = "FE Tools",
	Default = false,
	Callback = function(Value)
		loadstring(game:HttpGet(('https://pastefy.app/VYIAk3o1/raw'),true))()
	end    
})

Tab:AddButton({
	Name = "Rejoin Server",
	Default = false,
	Callback = function(Value)
		coroutine.resume(Rejoin)
	end    
})

